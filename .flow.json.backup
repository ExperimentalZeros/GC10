    {
        "id": "3fda2b903cba6a08",
        "type": "function",
        "z": "35aed2f02f81b42e",
        "name": "CHART CPM EXTRACTOR",
        "func": "//\"CHART\" PAYLOAD FORMAT\n//msg.payload[0].data[0] <----- Array\n//msg.payload[0].data[0][0] <-- Object\n//msg.payload[0].data[0][999]\n\n//Extract single \"y\" values\n//msg.payload = msg.payload[0].data[0].slice(-1)[0].y\n\n// Extract all \"y\" array values\nlet DATA = msg.payload[0].data[0]\n\nlet ARRAY = DATA.map(extract => extract.y).flat()\n\nlet MAX = Math.max(...ARRAY);\nlet MIN = Math.min(...ARRAY);\nlet AVG = Number ( ((MAX + MIN) / 2).toFixed(4) );\n\nmsg = [\n    {\"payload\": MAX, \"topic\": \"MAX\"},\n    {\"payload\": MIN, \"topic\": \"MIN\"},\n    {\"payload\": AVG, \"topic\": \"AVG\"},\n];\n\nreturn msg",
